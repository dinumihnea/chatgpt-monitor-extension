{
  "name": "chatgpt-monitor",
  "version": "0.0.1",
  "description": "ChatGPT Monitor - Experimental - Monitors and alerts about insecure ChatGPT prompts",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "webpack --config webpack.config.cjs --mode=production",
    "dev": "webpack --config webpack.config.cjs --mode=development --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "prettier": "prettier --write src/**/*.{ts,tsx,json,css,md}",
    "typecheck": "tsc --noEmit --skipLibCheck"
  },
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@types/chrome": "^0.1.12",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "copy-webpack-plugin": "^13.0.1",
    "css-loader": "^7.1.2",
    "eslint": "^9.35.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.4.0",
    "html-webpack-plugin": "^5.6.4",
    "prettier": "^3.6.2",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.2",
    "webpack": "^5.101.3",
    "webpack-cli": "^6.0.1"
  },
  "browserslist": {
    "production": [
      "chrome >= 88"
    ],
    "development": [
      "chrome >= 88"
    ]
  }
}
